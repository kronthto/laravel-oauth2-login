{
    "name": "kronthto/laravel-oauth2-login",
    "description": "Provides a middleware to protect resources requiring an OAuth2 login",
    "keywords": [
        "OAuth2",
        "Laravel",
        "Middleware",
        "ACL",
        "Login",
        "Session"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Kronthaler",
            "email": "kronthto@gmail.com"
        }
    ],
    "homepage": "https://github.com/kronthto/laravel-oauth2-login",
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.4.x|5.5.x|5.6.x|5.7.x",
        "league/oauth2-client": "^1.0.2|^2.0"
    },
    "require-dev": {
        "laravel/framework": ">=5.4.18",
        "orchestra/testbench": "~3.4|~3.5|~3.6",
        "orchestra/database": "~3.4|~3.5|~3.6",
        "phpunit/phpunit": "^5.7|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kronthto\\LaravelOAuth2Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --verbose"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kronthto\\LaravelOAuth2Login\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
